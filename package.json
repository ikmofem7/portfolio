{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "ikram portfolio",
  "scripts": {
    "start": "parcel src/index.html",
    "build": "rm -rf dist && parcel build src/index.html",
    "rimraf": "rm -rf node_modules/ .parcel-cache dist",
    "lint": "eslint --fix \"./src/**/*.{js,jsx,ts,tsx,json}\"",
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "runChecks": "yarn lint && yarn format",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ikmofem7/portfolio.git"
  },
  "keywords": [
    "portfolio"
  ],
  "author": "mohamed ikram",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ikmofem7/portfolio/issues"
  },
  "homepage": "https://github.com/ikmofem7/portfolio#readme",
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "parcel": "^2.8.2",
    "prettier": "^2.8.2",
    "process": "^0.11.10",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.6.1",
    "styled-components": "^5.3.6"
  }
